{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": ".",
    "paths":{"@/*":["*"]},
    "outDir": "./dist",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/Nav.js", "components/Layout.js", "components/Home.js", "components/Header.js", "components/Footer.js"],
  "exclude": ["node_modules","**/*.spec.ts"]
}
